@model ApiBD.Models.TbIndRedsocial

@{
    ViewData["Title"] = "Editar Red Social";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<div class="content-wrapper">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex justify-content-between">
                        <h4 class="card-title">@ViewData["Title"]</h4>
                    </div>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Nombre" class="control-label"></label>
                <input asp-for="Nombre" class="form-control" />
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clasehead" class="control-label"></label>
                <input asp-for="Clasehead" class="form-control" disabled />
                <span asp-validation-for="Clasehead" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ruta" class="control-label"></label>
                <input asp-for="Ruta" class="form-control" />
                <span asp-validation-for="Ruta" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clasefoot" class="control-label"></label>
                                    <input asp-for="Clasefoot" class="form-control" disabled />
                <span asp-validation-for="Clasefoot" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Icono" class="control-label"></label>
                                    <input asp-for="Icono" class="form-control" disabled />
                <span asp-validation-for="Icono" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Color" class="control-label"></label>
                                    <input asp-for="Color" class="form-control" disabled />
                <span asp-validation-for="Color" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Estado" class="control-label"></label>
                <input asp-for="Estado" class="form-control" />
                <span asp-validation-for="Estado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                                    <a class="btn btn-danger " asp-action="Index"><i class="ti-back-left"></i> Regresar</a>
            </div>
                            </form>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>