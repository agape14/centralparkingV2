@model ApiBD.Models.TbConfBotone

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<div class="content-wrapper">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex justify-content-between">
                        <h4 class="card-title">@ViewData["Title"]</h4>
                        <h3>¿Estas seguro de eliminar?</h3>
                    </div>

                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.MenuId)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.MenuId)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.PaginadetId)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.PaginadetId)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.BtnTitulo)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.BtnTitulo)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.BtnRuta)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.BtnRuta)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.BtnClase)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.BtnClase)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Icono)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Icono)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Orden)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Orden)
                        </dd>
                    </dl>

                    <form asp-action="Delete">
                        <input type="hidden" asp-for="Id" />
                        <button type="submit" class="btn btn-primary"><i class="ti-save"></i> Grabar</button>  |
                        <a class="btn btn-danger" href="@Url.Action("Index", "BotonCms", new { codigo = ViewBag.SlideCodigo })"><i class="ti-back-left"></i> Regresar</a>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>