@model CentralParkingSystem.ViewModel.ModalCombinadoCabDet;
@if (Model.listModalDet.Count!= 0)
{

    <div class="modal fade" id="modal_comprobantes" tabindex="-1" aria-labelledby="modalcomprobantes" aria-hidden="true">
        <div class="modal-dialog modal-lg modal-dialog-centered">
            <div class="modal-content rounded-1">
                <div class="modal-header ">
                    <h3 class="modal-title" id="modalcomprobantes">@Model.modalCab.Titulo</h3>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="accordion" id="accordionComprobantes">
                        <div class="accordion-item wow fadeInUp" data-wow-delay="0.1s">
                            <h2 class="accordion-header" id="headingCoprob1">
                                <button class="accordion-button collapsed" type="button"
                                        data-bs-toggle="collapse" data-bs-target="#collapseComprob1"
                                        aria-expanded="true" aria-controls="collapseComprob1">
                                    @Model.modalCab.Titulo
                                </button>
                            </h2>
                            <div id="collapseComprob1" class="accordion-collapse collapse"
                                 aria-labelledby="headingCoprob1" data-bs-parent="#accordionComprobantes">
                                <div class="accordion-body">

                                    <ul class="list-group">
                                        @foreach (var item in Model.listModalDet)
                                        {
                                            <li class="list-group-item"><a target="_blank" href="@item.Link"><i class="fa fa-file-invoice-dollar me-1"></i>@item.Titulo</a> </li>

                                        }
                                    </ul>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
}