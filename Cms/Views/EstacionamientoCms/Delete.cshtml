@model ApiBD.Models.TbEstacionamiento

@{
    ViewData["Title"] = "Eliminar";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<div class="content-wrapper">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex justify-content-between">
                        <h4 class="card-title">Eliminar Estacionamiento</h4>
                        <h3>¿Estas seguro de eliminar?</h3>
                    </div>

                    <dl class="row">
                        <dt class="col-sm-2">
                            Distrito
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.TbConfUbigeo.Dist)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Estacionamiento)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Estacionamiento)
                        </dd>
                        
                    </dl>
                    <form asp-action="Delete">
                        <input type="hidden" asp-for="Id" />
                        <button type="submit" class="btn btn-primary"><i class="ti-save"></i> Eliminar</button> |
                        <a class="btn btn-danger " href="@Url.Action("Index", "EstacionamientoCms", new { codUbi = @Model.CodUbi })"><i class="ti-back-left"></i> Regresar</a>
                    </form>

                </div>
            </div>
        </div>
    </div>
</div>