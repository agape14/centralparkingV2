@model ApiBD.Models.TbFormProveedore

@{
    ViewData["Title"] = "Editar detalle de pagina";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<div class="content-wrapper">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex justify-content-between">
                        <h4 class="card-title">@ViewData["Title"]</h4>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <form asp-action="Edit">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="hidden" asp-for="Id" />

                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Fecha" class="control-label"></label>
                                            <input asp-for="Fecha" class="form-control" />
                                            <span asp-validation-for="Fecha" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="SolicitadoPor" class="control-label"></label>
                                            <input asp-for="SolicitadoPor" class="form-control" />
                                            <span asp-validation-for="SolicitadoPor" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FRuc" class="control-label"></label>
                                            <input asp-for="FRuc" class="form-control" />
                                            <span asp-validation-for="FRuc" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FDni" class="control-label"></label>
                                            <input asp-for="FDni" class="form-control" />
                                            <span asp-validation-for="FDni" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FNombre" class="control-label"></label>
                                            <input asp-for="FNombre" class="form-control" />
                                            <span asp-validation-for="FNombre" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FDireccion" class="control-label"></label>
                                            <input asp-for="FDireccion" class="form-control" />
                                            <span asp-validation-for="FDireccion" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FDistrito" class="control-label"></label>
                                            <input asp-for="FDistrito" class="form-control" />
                                            <span asp-validation-for="FDistrito" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FTelefono" class="control-label"></label>
                                            <input asp-for="FTelefono" class="form-control" />
                                            <span asp-validation-for="FTelefono" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FRubro" class="control-label"></label>
                                            <input asp-for="FRubro" class="form-control" />
                                            <span asp-validation-for="FRubro" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FTipoPersona" class="control-label"></label>
                                            <input asp-for="FTipoPersona" class="form-control"  disabled/>
                                            <span asp-validation-for="FTipoPersona" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FCodigoPostal" class="control-label"></label>
                                            <input asp-for="FCodigoPostal" class="form-control" />
                                            <span asp-validation-for="FCodigoPostal" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="FFax" class="control-label"></label>
                                            <input asp-for="FFax" class="form-control" />
                                            <span asp-validation-for="FFax" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="CcNombre" class="control-label"></label>
                                            <input asp-for="CcNombre" class="form-control" />
                                            <span asp-validation-for="CcNombre" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="CcCargo" class="control-label"></label>
                                            <input asp-for="CcCargo" class="form-control" />
                                            <span asp-validation-for="CcCargo" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="CcCelular" class="control-label"></label>
                                            <input asp-for="CcCelular" class="form-control" />
                                            <span asp-validation-for="CcCelular" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="CcTelefono" class="control-label"></label>
                                            <input asp-for="CcTelefono" class="form-control" />
                                            <span asp-validation-for="CcTelefono" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="CcEmail" class="control-label"></label>
                                            <input asp-for="CcEmail" class="form-control" />
                                            <span asp-validation-for="CcEmail" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-primary"><i class="ti-save"></i> Grabar</button> |
                                    <a class="btn btn-danger " asp-action="Index"><i class="ti-back-left"></i> Regresar</a>
                                </div>
                            </form>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>