@model ApiBD.Models.TbFormHojareclamacione

@{
    ViewData["Title"] = "Nuevo Hoja Reclamación";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}


<div class="content-wrapper">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex justify-content-between">
                        <h4 class="card-title">@ViewData["Title"]</h4>
                    </div>


                    <div class="row">
                        <div class="col-md-12">
                            <form enctype="multipart/form-data" method="post" asp-action="Create">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Fechaindicente" class="control-label"></label>
                                            <input asp-for="Fechaindicente" class="form-control" type="date" />
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Nombre" class="control-label"></label>
                                            <input asp-for="Nombre" class="form-control" />
                                            <span asp-validation-for="Nombre" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Apellido" class="control-label"></label>
                                            <input asp-for="Apellido" class="form-control" />
                                            <span asp-validation-for="Apellido" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Correo" class="control-label"></label>
                                            <textarea asp-for="Correo" class="form-control" rows="4" cols="50" resize="both"></textarea>
                                            <span asp-validation-for="Correo" class="text-danger"></span>
                                        </div>
                                    </div>



                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Telefono" class="control-label"></label>
                                            <input asp-for="Telefono" class="form-control" />
                                            <span asp-validation-for="Telefono" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Telefono" class="control-label"></label>
                                            <input asp-for="Telefono" class="form-control" />
                                            <span asp-validation-for="Telefono" class="text-danger"></span>
                                        </div>
                                    </div>


                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Tipodocumento" class="control-label"></label>
                                            <input asp-for="Tipodocumento" class="form-control" type="number" />
                                            <span asp-validation-for="Tipodocumento" class="text-danger"></span>
                                        </div>
                                    </div>



                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Numerodocumento" class="control-label"></label>
                                            <input asp-for="Numerodocumento" class="form-control" />
                                            <span asp-validation-for="Numerodocumento" class="text-danger"></span>
                                        </div>
                                    </div>



                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Departamento" class="control-label"></label>
                                            <input asp-for="Departamento" class="form-control" />
                                            <span asp-validation-for="Departamento" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Provincia" class="control-label"></label>
                                            <input asp-for="Provincia" class="form-control" type="number" />
                                            <span asp-validation-for="Provincia" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Distrito" class="control-label"></label>
                                            <input asp-for="Distrito" class="form-control" type="number" />
                                            <span asp-validation-for="Distrito" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Direccion" class="control-label"></label>
                                            <input asp-for="Direccion" class="form-control" type="number" />
                                            <span asp-validation-for="Direccion" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Reclamo" class="control-label"></label>
                                            <input asp-for="Reclamo" class="form-control" type="number" />
                                            <span asp-validation-for="Reclamo" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Tiposervicio" class="control-label"></label>
                                            <input asp-for="Tiposervicio" class="form-control" type="number" />
                                            <span asp-validation-for="Tiposervicio" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Estacionamiento" class="control-label"></label>
                                            <input asp-for="Estacionamiento" class="form-control" type="number" />
                                            <span asp-validation-for="Estacionamiento" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Detalle" class="control-label"></label>
                                            <input asp-for="Detalle" class="form-control" type="number" />
                                            <span asp-validation-for="Detalle" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    @*<input type="submit" value="Create" class="btn btn-primary" />*@
                                    <button type="submit" class="btn btn-primary"><i class="ti-save"></i> Grabar</button> |
                                    <a class="btn btn-danger" asp-action="Index" asp-route-codigo="@ViewBag.vServicioId"><i class="ti-back-left"></i> Regresar</a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
